<h1>Houseplants</h1>
<h3>Register below to organize the plants around your home!</h3>

<%= form_for @user do |f| %>
  First Name:
  <br>
  <%= f.text_field :first_name %>
  <br>
  Last Name:
  <br>
  <%= f.text_field :last_name %>
  <br>
  Email:
  <br>
  <%= f.text_field :email %>
  <br>
  Password:
  <br>
  <%= f.password_field :password_digest %>
  <br>
  <br>
  <%= f.submit%>
<% end %>